<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pidev.esprit.Controllers.Commentaire.CommentListController">
    <children>
        <TableView fx:id="commentTableView" layoutX="25.0" layoutY="8.0" prefHeight="315.0" prefWidth="550.0">
            <columns>
                <TableColumn fx:id="idCommentColumn" prefWidth="96.79998779296875" text="Id_comment">
                    <cellValueFactory>
                        <PropertyValueFactory property="id_commentaire" />
                    </cellValueFactory>
                </TableColumn>

                <TableColumn fx:id="idProjetColumn" prefWidth="121.60003662109375" text="Id_project">
                    <cellValueFactory>
                        <PropertyValueFactory property="id_projet" />
                    </cellValueFactory>
                </TableColumn>

                <TableColumn fx:id="contentColumn" prefWidth="132.79998779296875" text="Content">
                    <cellValueFactory>
                        <PropertyValueFactory property="contenue" />
                    </cellValueFactory>
                </TableColumn>

                <TableColumn fx:id="dateColumn" prefWidth="150" text="Date">
                    <cellValueFactory>
                        <PropertyValueFactory property="date_commentaire" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
      <Button layoutX="203.0" layoutY="341.0" mnemonicParsing="false" onAction="#deleteComment" prefHeight="38.0" prefWidth="187.0" stylesheets="@Stylesheet/Style.css" text="Delete Commentaire">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="12.0" />
         </font>
         <styleClass>
            <String fx:value="my-button" />
            <String fx:value="button" />
         </styleClass></Button>
    </children>
</AnchorPane>
